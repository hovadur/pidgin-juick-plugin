all:
	sh libtool --silent --mode=compile gcc -DHAVE_CONFIG_H \
	-I../.. \
	-DDATADIR=\"/usr/share\" \
	-I../../libpurple \
	-I../../pidgin \
	-Wall  -DPURPLE_DISABLE_DEPRECATED -DPIDGIN_DISABLE_DEPRECATED -DFINCH_DISABLE_DEPRECATED -DGNT_DISABLE_DEPRECATED -Waggregate-return -Wcast-align -Wdeclaration-after-statement -Wendif-labels -Werror-implicit-function-declaration -Wextra -Wno-sign-compare -Wno-unused-parameter -Wformat-security -Winit-self -Wmissing-declarations -Wmissing-noreturn -Wmissing-prototypes -Wpointer-arith -Wundef -Wp, \
	-D_FORTIFY_SOURCE=2 \
	-I/usr/include/gtk-2.0 \
	-I/usr/lib/gtk-2.0/include \
	-I/usr/include/atk-1.0 \
	-I/usr/include/cairo \
	-I/usr/include/pango-1.0 \
	-I/usr/include/glib-2.0 \
	-I/usr/lib/glib-2.0/include \
	-I/usr/include/pixman-1 \
	-I/usr/include/freetype2 \
	-I/usr/include/libpng12 \
	-g -g -O2 -c juick.c -o tmpjuick.so.lo
	sh libtool --silent --mode=link    gcc -g -g -O2 -o libtmpjuick.so.la -rpath \
	/usr/lib/pidgin tmpjuick.so.lo -lutil -lpthread -lnsl -lresolv   -module -avoid-version
	@rm -f tmpjuick.* libtmpjuick.*
	@cp .libs/libtmpjuick.so.so juick.so
	@rm -f .libs/libtmpjuick*
	strip juick.so